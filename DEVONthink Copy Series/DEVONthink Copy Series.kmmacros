						<string>tell application "DEVONthink"		set frontWindow to front window	try		set currentItem to content record of frontWindow		set currentName to name of currentItem		-- 将"《"替换为"〈"，"》"替换为"〉"		set currentName to do shell script "echo " &amp; quoted form of currentName &amp; " | sed -e 's/《/〈/g' -e 's/》/〉/g'"		set Hierarchy to get custom meta data for "效力位阶" from currentItem		set Status to get custom meta data for "时效性" from currentItem		set EDate to get custom meta data for "生效日期" from currentItem		set Num to get custom meta data for "发文字号" from currentItem		set output to ("《" &amp; currentName &amp; "（" &amp; Num &amp; "）》 " &amp; Hierarchy &amp; " " &amp; EDate &amp; "生效 " &amp; Status) as string	on error		set currentSelection to selection of frontWindow		if the (count of currentSelection) &gt; 1 then			set URLList to {}			set nameList to {}			set HierarchyList to {}			set StatusList to {}			set EdateList to {}			set numList to {}			set metadataList to {}						repeat with this_record in currentSelection				set currentName to (name of this_record as string)				-- 将"《"替换为"〈"，"》"替换为"〉"				set currentName to do shell script "echo " &amp; quoted form of currentName &amp; " | sed -e 's/《/〈/g' -e 's/》/〉/g'"				set the end of nameList to currentName				set the end of HierarchyList to (get custom meta data for "效力位阶" from this_record)				set the end of StatusList to (get custom meta data for "时效性" from this_record)				set the end of EdateList to (get custom meta data for "生效日期" from this_record)				set the end of numList to (get custom meta data for "发文字号" from this_record)			end repeat						repeat with t from 1 to the count of nameList				set the end of metadataList to (t &amp; ". 《" &amp; (item t of nameList) &amp; "（" &amp; (item t of numList) &amp; "）》 " &amp; (item t of HierarchyList) &amp; " " &amp; (item t of EdateList) &amp; "生效 " &amp; (item t of StatusList)) as string			end repeat						set old_delim to AppleScript's text item delimiters			set AppleScript's text item delimiters to return			set output to metadataList as string			set AppleScript's text item delimiters to old_delim					else			set currentItem to item 1 of currentSelection			set currentName to name of currentItem			-- 将"《"替换为"〈"，"》"替换为"〉"			set currentName to do shell script "echo " &amp; quoted form of currentName &amp; " | sed -e 's/《/〈/g' -e 's/》/〉/g'"			set Hierarchy to get custom meta data for "效力位阶" from currentItem			set Status to get custom meta data for "时效性" from currentItem			set EDate to get custom meta data for "生效日期" from currentItem			set Num to get custom meta data for "发文字号" from currentItem			set output to ("《" &amp; currentName &amp; "（" &amp; Num &amp; "）》 " &amp; Hierarchy &amp; " " &amp; EDate &amp; "生效 " &amp; Status) as string		end if			end tryend tellset the clipboard to output</string>
