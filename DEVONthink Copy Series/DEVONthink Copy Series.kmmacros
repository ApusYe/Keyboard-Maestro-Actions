						<string>tell application "DEVONthink"	set frontWindow to front window	try		set currentItem to content record of frontWindow		set currentName to name of currentItem		set currentItemlink to reference URL of currentItem		set output to ("[" &amp; currentName &amp; "](" &amp; currentItemlink &amp; ")") as string	on error		set currentSelection to selection of frontWindow		if the (count of currentSelection) &gt; 1 then			set itemlinklist to {}			set nameList to {}			set mdlinkList to {}						repeat with this_record in currentSelection				set the end of itemlinklist to (reference URL of this_record as string)				set the end of nameList to (name of this_record as string)			end repeat						repeat with t from 1 to the count of nameList				set the end of mdlinkList to ("- [" &amp; (item t of nameList) &amp; "](" &amp; (item t of itemlinklist) &amp; ")")			end repeat						set old_delim to AppleScript's text item delimiters			set AppleScript's text item delimiters to return			set output to mdlinkList as string			set AppleScript's text item delimiters to old_delim					else			set currentName to name of item 1 of currentSelection			set currentItemlink to reference URL of item 1 of currentSelection			set output to ("[" &amp; currentName &amp; "](" &amp; currentItemlink &amp; ")") as string		end if			end tryend tellset the clipboard to output</string>
